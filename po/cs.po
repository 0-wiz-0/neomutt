"  neomutt [-n] [-e <command>] [-F <config>] -Q <variable>\n"
"  neomutt [-n] [-e <command>] [-F <config>] -Z\n"
"  neomutt [-n] [-e <command>] [-F <config>] -z [-f <mailbox>]\n"
